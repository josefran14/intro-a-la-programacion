Algoritmo arreglo
	    Escribir "Ingresa el tamaño de la dimension";
	    Leer tam;
		
		Dimension numes[tam];
		
		Para x <-1 Hasta tam Con Paso 1 Hacer
			
			Escribir "Ingresa un dato para la posicion : ",x;
			Leer numes[x];
			
		Fin Para
		
		Escribir "El contenido del arreglo es: ";
		Para i<-1 Hasta tam Con Paso 1 Hacer
			Escribir numes[i];
		Fin Para
		
FinAlgoritmo
	

